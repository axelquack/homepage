# .env file for JDownloader service with Homepage auto-discovery

## Server IP or Hostname
SERVER_IP=<your_ip>  # Replace with your NAS IP or hostname

# User and Group IDs
PUID=<your_id>
PGID=<your_id>

# Timezone
TZ=<your_timezone>

# Web Authentication
WEB_USER=<your_id>
WEB_PASSWORD=<your_password>

# Ports
WEB_PORT=<your_port>

# Volume Paths
JD_CONFIG_PATH=/DATA/AppData/jdownloader/config
DOWNLOAD_PATH=<your_path>

# Homepage Labels (for Dockge or similar interfaces)
HOMEPAGE_GROUP=Tools
HOMEPAGE_NAME=JDownloader
HOMEPAGE_ICON=https://raw.githubusercontent.com/homarr-labs/dashboard-icons/refs/heads/main/png/jdownloader.png
HOMEPAGE_HREF=http://${SERVER_IP}:${WEB_PORT}
HOMEPAGE_DESCRIPTION=FTP/HTTP Download CLient

# MyJDownloader Credentials
MYJD_USERNAME=<your_myjd_id>
MYJD_PASSWORD=<your_myjd_password>
MYJD_CLIENT=<your_client_id>
